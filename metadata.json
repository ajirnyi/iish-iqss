{
    "name": "iish-iqss",
    "version": "0.1.0",
    "author": "Lucien van Wouw",
    "summary": "This module will install and configure the IQSS Dataverse application.",
    "license": "GPLv3",
    "source": "https://github.com/iish/iish-iqss/",
    "project_page": "https://github.com/iish/iish-iqss/",
    "issues_url": "https://github.com/iish/iish-iqss/issues",
    "dependencies": [
        {
            "name": "puppetlabs-postgresql",
            "version_requirement": ">=4.3.0"
        },
        {
            "name": "puppetlabs-apache",
            "version_requirement": ">=1.5.0"
        },
        {
            "name": "rfletcher-jq",
            "version_requirement": ">=0.0.2"
        },
        {
            "name": "jefferyb-shibboleth",
            "version_requirement": ">=0.3.1"
        },
        {
            "name": "camptocamp-archive",
            "version_requirement": ">=0.7.4"
        }
    ],
    "operatingsystem_support": [
        {
            "operatingsystem": "CentOS",
            "operatingsystemrelease": [
                "6"
            ]
        },
        {
            "operatingsystem": "Ubuntu",
            "operatingsystemrelease": [
                "12.04",
                "14.04"
            ]
        }
    ]
}

